<p id="notice"><%= notice %></p>

<h1>Student Marks</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Sem1</th>
      <th>Sem2</th>
      <th>Sem3</th>
      <th>Sem4</th>
      <th>Sem5</th>
      <th>Sem6</th>
      <th>Sem7</th>
      <th>Sem8</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_marks.each do |student_mark| %>
      <tr>
        <td><%= student_mark.student_id %></td>
        <td><%= student_mark.sem1 %></td>
        <td><%= student_mark.sem2 %></td>
        <td><%= student_mark.sem3 %></td>
        <td><%= student_mark.sem4 %></td>
        <td><%= student_mark.sem5 %></td>
        <td><%= student_mark.sem6 %></td>
        <td><%= student_mark.sem7 %></td>
        <td><%= student_mark.sem8 %></td>
        <td><%= link_to 'Show', student_mark %></td>
        <td><%= link_to 'Edit', edit_student_mark_path(student_mark) %></td>
        <td><%= link_to 'Destroy', student_mark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Mark', new_student_mark_path %>
